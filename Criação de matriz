#include<iostream.h>                                    
#include<iomanip.h>
 main()
{    int M[20] [20] ,n,m,i,j;
	cout<<"numero de linhas=";
	cin>>n;
	cout<<"numero de colunas=";
	cin>>m;
	for(i=0; i<n; i++)
	  for(j=0; j<m; j++)
			  { cout<<"m["<<(i+1)<<","<<(j+1)<<"]=";
				 cin>>M[i][j];
				 }
	 cout<<"\n\n";
	 for(i=0;i<n;i++)
		{ for(j=0;j<m;j++)
			  cout<<setw(8)<<M[i][j];
				cout<<"\n";
            }
system("pause>null");
				 }
